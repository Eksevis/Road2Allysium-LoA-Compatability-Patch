[t:interface,n:console_output]
{
	  intended_resolution_height = 480;
	  intended_resolution_width = 640;

	[t:chat_box,n:console_box]
	{
		f alpha = 1.0000000;
		b common_control = false;
		  common_template = ;
		  console = true;
		i draw_order = 0;
		  font_color = 0xffdfff00;//0xffffffff
		i font_size = 15;
		  font_type = b_gui_fnt_14p_copperplate-light;
		  font_1024x768 = b_gui_fnt_14p_copperplate-light;
		  font_800x600 = b_gui_fnt_14p_copperplate-light;
		  font_640x480 = b_gui_fnt_12p_copperplate-light;
		b has_slider = true;
		  justify = center;
		f line_timeout = 10.000000;
		i max_string_size = 0;
		  pass_through = true;
		  rect = 152,6,551,137;
		b scroll_down = true;
		  text = "";
		  texture = b_gui_cmn_box_alpha_154;
		  uvcoords = 0.000000,0.000000,1.000000,1.000000;
		  wrap_mode = clamp;
		  delete_timeouts = false;
		i max_history = 100;
		b stretch_x = false;
		  center_x = true;
		b stretch_y = false;
	}

	[t:window;n:rollover_console]
	{
		i draw_order = 1;
		b pass_through = true;
		b stretch_x = true;
		b stretch_y = true;
		rect = 152,6,551,21;

		[messages]
		{
			onrollover = notify(console_expand);
			onrolloff = notify(console_shrink);
		}
	}
}